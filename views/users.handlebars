<div class="container">
    <div class="row">
        {{#each users}}
        <div class="col-sm-4" >
            <div class="card">
                <img src={{image}} class="card-img-top">
                <div class="card-body">
                <a href="/user/{{id}}" class="card-text">{{fullName}}</a>
                <p class="card-text">{{location}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>